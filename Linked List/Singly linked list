#include <stdio.h>
#include <stdlib.h>
/* Node structure */
struct node {
    int data;
    struct node *next;
};
struct node *head = NULL;
/* Insert at end */
void insert(int data) {
    struct node *newnode, *temp;
    newnode = (struct node *)malloc(sizeof(struct node));
    newnode->data = data;
    newnode->next = NULL;
    if (head == NULL) {
        head = newnode;
        return;
    }
    temp = head;
    while (temp->next != NULL) {
        temp = temp->next;
    }
    temp->next = newnode;
}
/* Display linked list */
void display() {
    struct node *temp = head;

    if (temp == NULL) {
        printf("List is empty");
        return;
    }
    while (temp != NULL) {
        printf("%d -> ", temp->data);
        temp = temp->next;
    }
    printf("NULL");
}
/* Main function */
int main() {
    int n, data;
    printf("Enter number of nodes: ");
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        printf("Enter data: ");
        scanf("%d", &data);
        insert(data);
    }
    printf("\nLinked List:\n");
    display();
    return 0;
}
